# macOS
.DS_Store

# Node modules (ignore at any level)
node_modules/

# Environment files (ignore all except .sample)
.env*
!.env.sample

# Generated iOS/Android folders
/ios
/android

# iOS app directory
/ios-app/.expo
# /ios-app/ios  # UNIGNORED: Needed for Dynamic Island Widget Extension in EAS builds
/ios-app/ios/Pods/  # Ignore CocoaPods dependencies (generated from Podfile)
/ios-app/ios/build/  # Ignore build artifacts
/ios-app/android

# Database exports & schema files (root level only)
/*.schema.txt
/*.schema.sql

# Supabase - ignore generated/temp files only
/supabase/types.d.ts
/supabase/fingerprint-compare.ts
/supabase/schema_fingerprint.sql

# Root level files that should stay in ios-app/
/package.json
/package-lock.json
/tsconfig.json

# Backups
*.backup
