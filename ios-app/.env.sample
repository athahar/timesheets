# Expo environment configuration
# Copy this file to `.env` and replace the placeholders below.

# Supabase project URL (e.g. https://your-project.supabase.co)
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co

# Supabase anon/public API key (never commit the real value)
EXPO_PUBLIC_SUPABASE_ANON_KEY=replace-with-supabase-anon-key

# App display name exposed to Expo
EXPO_PUBLIC_APP_NAME=TrackPay

# Runtime environment tag (development | staging | production)
EXPO_PUBLIC_ENV=development
