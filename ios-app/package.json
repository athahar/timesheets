{
  "name": "timesheets-app",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "start:dev": "cp .env.development .env && expo start",
    "start:dev:clear": "cp .env.development .env && expo start --clear",
    "start:staging": "cp .env.staging .env && expo start",
    "start:staging:clear": "cp .env.staging .env && expo start --clear",
    "start:prod": "mv .env.development .env.development.backup 2>/dev/null || true && cp .env.production .env && expo start",
    "start:prod:clear": "mv .env.development .env.development.backup 2>/dev/null || true && cp .env.production .env && expo start --clear",
    "web": "expo start --web",
    "web:dev": "cp .env.development .env && expo start --web",
    "web:dev:clear": "cp .env.development .env && expo start --web --clear",
    "web:staging": "cp .env.staging .env && expo start --web",
    "web:staging:clear": "cp .env.staging .env && expo start --web --clear",
    "web:prod": "mv .env.development .env.development.backup 2>/dev/null || true && cp .env.production .env && expo start --web",
    "web:prod:clear": "mv .env.development .env.development.backup 2>/dev/null || true && cp .env.production .env && expo start --web --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "env:dev": "mv .env.development.backup .env.development 2>/dev/null || true && cp .env.development .env && echo 'âœ… Switched to DEVELOPMENT environment'",
    "env:staging": "mv .env.development.backup .env.development 2>/dev/null || true && cp .env.staging .env && echo 'âœ… Switched to STAGING environment'",
    "env:prod": "mv .env.development .env.development.backup 2>/dev/null || true && cp .env.production .env && echo 'âœ… Switched to PRODUCTION environment'",
    "env:check": "echo 'ðŸ“‹ Current environment:' && grep EXPO_PUBLIC_ENV .env 2>/dev/null || echo 'No .env file' && grep EXPO_PUBLIC_SUPABASE_URL .env 2>/dev/null || true"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "@supabase/supabase-js": "^2.57.4",
    "babel-preset-expo": "^54.0.2",
    "dotenv": "^17.2.2",
    "expo": "54.0.13",
    "expo-font": "14.0.9",
    "expo-haptics": "^15.0.7",
    "expo-localization": "~17.0.7",
    "expo-status-bar": "~3.0.8",
    "i18next": "^25.5.2",
    "libphonenumber-js": "^1.12.22",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^15.7.3",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "typescript": "~5.9.2"
  },
  "private": true
}
