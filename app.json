{
  "expo": {
    "name": "TrackPay",
    "slug": "trackpay",
    "version": "0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.trackpay.track",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "TrackPay uses camera for profile photos and document scanning",
        "NSPhotoLibraryUsageDescription": "TrackPay accesses photo library for profile photos"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.trackpay.track",
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "trackpay",
    "platforms": ["ios", "android", "web"],
    "extra": {
      "eas": {
        "projectId": ""
      }
    }
  }
}
